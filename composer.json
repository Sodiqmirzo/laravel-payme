{
    "name": "ittech/laravel-payme",
    "description": "This is my package laravel-payme-client",
    "keywords": [
        "Ittech",
        "laravel",
        "laravel-payme",
        "payme",
        "laravel-payme-client",
        "payme-client"
    ],
    "homepage": "https://github.com/ittech/laravel-payme",
    "license": "MIT",
    "authors": [
        {
            "name": "Sodiqmirzo Sattorov",
            "email": "mr.sodiqmirzosattorov17@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-data": "^3.6",
        "spatie/laravel-http-logger": "^1.10",
        "spatie/laravel-package-tools": "^1.13.0",
        "illuminate/http": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ittech\\Payme\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false,
            "phpstan/extension-installer": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ittech\\Payme\\PaymeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
